<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name = "dominio.Prenotazione" table = "prenotazione">
      
      <meta attribute = "class-description">
        Questa classe contiene i dati delle prenotazioni
      </meta>
      <!-- Da mappare -->
      
      <property name ="id" column = "id_prenotazione" type = "int"/>
      <property name = "pagato" column = "pagato" type = "boolean"/>
      <property name = "prezzoTotale" column = "prezzo_totale" type = "double"/>
    
   </class>
</hibernate-mapping>