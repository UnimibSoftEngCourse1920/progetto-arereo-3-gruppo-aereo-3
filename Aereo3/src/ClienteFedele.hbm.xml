<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name = "dominio.ClienteFedele" table = "cliente">
      
      <meta attribute = "class-description">
        Questa classe contiene i dati di clienti
      </meta>
      
      
      <property name ="punti" column = "punti" type = "int"/>
      <property name = "dataIscrizione" column = "data_iscrizione" type = "date"/>
      <property name = "fedele" column = "fedele" type = "boolean"/>
      
      
   </class>
</hibernate-mapping>